{
    "manifest_version": 3,
    "name": "Intent Reminder",
    "version": "0.1",
    "description": "Helps you remember unfinished work and saved links.",
    "background": {
    "service_worker": "background.js"},
    "permissions": ["tabs", "storage", "activeTab", "notifications"],
    "action": {
      "default_popup": "popup.html"
    }
  }

  
  